<!DOCTYPE html>
<html>
  <head>
    <title>Tradeschool</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if !@outside_view %>
      <div id="navbar">
        <div class="wrapper">
          <nav class="clearfix">
            <%= link_to "All Users", users_path%> <%= link_to "My Profile", users_path%> <%= link_to "Edit my Profile", users_path%>

          </nav>
        </div><!-- .wrapper -->
      </div><!-- #navbar -->
    <% end %>

    <%= yield %>

  </body>
</html>
